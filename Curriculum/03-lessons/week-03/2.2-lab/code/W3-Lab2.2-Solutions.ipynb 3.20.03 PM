    "# Exercise 1 Code here\n",
    "\n",
    "for i, label1 in enumerate(df.columns):\n",
    "    for j, label2 in enumerate(df.columns):\n",
    "        # No need for repeats\n",
    "        if j >= i:\n",
    "            continue\n",
    "        X = df[[label1]]\n",
    "        y = df[label2]\n",
    "\n",
    "        model = lm.fit(X, y)\n",
    "        predictions = lm.predict(X)\n",
    "\n",
    "        print label1, label2, model.score(X,y)"
